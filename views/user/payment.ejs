<%- include('./include/header.ejs') %>
<style>
    .borderBox{
        border: 2px dashed #333;
        margin-bottom: 10px;
    }
</style>

</head  >

<body onload="loader()">
	<div id="loader"> </div>
	<!-- Header -->
	<%- include('./loginNavbar.ejs') %>
	<!-- Header / End -->
	<!-- side Navbar -->
	<%- include('./sideNavbar.ejs') %>
	<!-- side Navbar -->

	<div class="content">
		<div class="row">
			<div class="col-md-12">
				<h3>All Payment Information</h3>
				<hr>
			</div>
			<div class="col-md-12">
				<% if(booking.length) { %>
					<% for(let i=0; i<booking.length ; i++ ){  %>
				<div class="payment">
					<div class="media">
						<div class="media-body">
							<h5 class="mt-0 mb-1"><%=booking[i].businessName%></h5>
							<p><%=booking[i].serviceName%></p>
							<p><%=booking[i].day%></p>
						</div>
						<h4>Price: <%=booking[i].servicePrice%></h4>
					</div>
				</div>
				<% } %>
				<% } else{  %>
						 
					<div class="container">
						<div class="row">
							<div class="col-md-12">
								<h3 class="text-center">No Data Found</h3>
							</div>
						</div>
					</div>
					<%  } %>
			</div>
		</div>
	</div>
</div>
</div>

</section>

<%- include('./include/script.ejs') %>

</body>
</html>



