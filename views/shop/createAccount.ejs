          <p class="text-center">No credit card. No commitment. It takes only 2 minutes.</p>
          <input id="businessName" name="businessName" type="text" class="form-control bg-dark text-white" placeholder="What is the name of your Business"
            required>
          <input id="businessLink" name="businessLink" type="text" class="form-control bg-dark text-white" placeholder="business website" required>
          <input id="ownerName" name="ownerName" type="text" class="form-control bg-dark text-white" placeholder="What is your Name?" required>
          <input id="ownerEmail" name="ownerEmail" type="text" class="form-control bg-dark text-white" placeholder="What is your email address" required>
          <input id="password" name="password"  type="password" class="form-control bg-dark text-white" placeholder="set your booksy password" required>
          <a id="loginInfobtn"  class="btn btn-info text-center loginbtn text-dark " value="Create your booksy account">Create
            your
            booksy account</a>
        </div>
      </div>
    </div>
  </section>

  <!-- login info end -->

