          <div class="addService text-center" data-toggle="modal" data-target="#addServiceModalLong">
            <a href="#">ADD Service</a>
          </div>
          <div class="addCategory text-center" data-toggle="modal" data-target="#addCategoryModalLong">
            <a href="#">ADD Category</a>
          </div>
        </div>
      </div>

  
    

      <!-- add Service modal start -->
      <div class="modal fade" id="addServiceModalLong" tabindex="-1" role="dialog"
        aria-labelledby="exampleModalLongTitle" aria-hidden="true">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title">Edit service</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body">
              <div class="service-name">
                <label>Service name</label>
                <input type="text" class="form-control" id="serviceName" placeholder="Service Name">
              </div>
              <div class="price-option">
                <h2>Price Options</h2>
                <p>Duration</p>
                <div class="row">
                  <select class="col-6 form-control" id="hour">
                    <option value="1">1 <span>hour(s)</span> </option>
                  </select>
                  <select class="col-6 form-control" id="min">
                    <option value="1">1 <span>min(s)</span> </option>
                  </select>
                </div>
                <p>Price Type & Price</p>
                <div class="row">
                  <select class="col-6 form-control" id="priceType">
                    <option value="Fixed">Fixed</option>
                    <option value="Varies">Varies</option>
                    <option value="Dont show">Don't show</option>
                  </select>
                  <input type="text" id="price" class="col-6 form-control">
                </div>

              </div>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-primary close"  data-dismiss="modal" aria-label="Close" onclick="addNewCatandTime()">Save changes</button>
            </div>
          </div>
        </div>
      </div>
      <!-- add Service modal end -->

      <!-- add category start -->
      <div class="modal fade" id="addCategoryModalLong" tabindex="-1" role="dialog"
        aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title">Add Category</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body">
              <input id="addCategory" type="text" class="form-control" placeholder="Add category">
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-primary close" data-dismiss="modal" onclick="addNewCategory()" aria-label="Close">Save changes</button>
              <button type="button" class="btn btn-primary" onclick="addNewCategory()">Save and another</button>
            </div>
          </div>
        </div>
      </div>
      <!-- add category end -->

    </div>
  </section>
  <!-- service and price section end -->


  <div id="finishDiv">
    <button class="btn btn-danger" type="submit" id="finishButton">Finish</button>
  </div>

