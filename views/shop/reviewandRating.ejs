<!DOCTYPE html>
<html dir="ltr" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="../assets/images/favicon.png">
    <title>Material pro admin Template - The Ultimate Multipurpose admin template</title>
    <link rel="canonical" href="https://www.wrappixel.com/templates/Material proadmin/" />
    <link rel="stylesheet" href="/dist/css/icons/font-awesome/css/fontawesome.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Custom CSS -->
    <link rel="stylesheet" href="/assets/extra-libs/taskboard/css/lobilist.css">
    <link rel="stylesheet" href="/assets/extra-libs/taskboard/css/jquery-ui.min.css">
    <link rel="stylesheet" type="text/css"
        href="/assets/libs/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
    <!-- Custom CSS -->
    <link href="/dist/css/style.min.css" rel="stylesheet">
    <style>
        .reviewBox {
            border: 2px solid rgb(15, 14, 14);
            padding: 10px;
            box-shadow: 0px 4px 2px 1px black;
        }

        .reviewBox ul li {
            float: left;
            list-style: none;
        }
    </style>

</head>

<body>

    <%- include('./sideBar.ejs') %>

    <!-- Page wrapper  -->
    <!-- ============================================================== -->
    <div class="page-wrapper">
        <div class="heath-information">
            <div class="container">
                <h1 class="text-center">Review Rating</h1>
                <div class="row">
                    <% for(let i=0; i< data.length; i++ ) { %>

                    <div class="col-md-6">
                        <div class="reviewBox">
                            <div class="row">
                                <div class="col-md-6">
                                    <h4><img class="reviewDp" src="<%=data[i].reviewrDP%>" alt="dp">
                                        <span><%=data[i].reviewrName%></span></h4>
                                </div>
                                <div class="col-md-6">
                                    <ul>
                                        <% for(let j=0 ;j < data[i].rating ; j++ ) { %>
                                        <li><i class="fa fa-star"></i></li>
                                        <% } %>
                                    </ul>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <img class="reviewImg" src="<%=data[i].img%>" alt="">
                                </div>
                                <div class="col-md-12">
                                    <p class="mt-1"><%=data[i].review%> </p>
                                </div>
                            </div>
                            <div>
                                    <span><%=data[i].date%></span>
                            </div>
                        </div>
                    </div>
                    

                    <% } %>
                </div>
            </div>
        </div>
    </div>

    </div>
    <!-- ============================================================== -->
    <!-- End Page wrapper  -->
    <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->

 <!-- JS Plugin Start-->
 <%- include('./include/js.ejs') %>
  <!-- JS Plugin End-->

</body>

</html>